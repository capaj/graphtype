{
  "name": "gql-tsqb",
  "version": "1.0.0",
  "description": "GraphQL query builder for typescript. Write GQL queries as objects so that TS compiler can typecheck your queries",
  "keywords": [
    "graphql",
    "typescript",
    "utility"
  ],
  "repository": "git@github.com:capaj/gql-tsqb",
  "contributors": [
    "gosho-kazuya <ketsume0211@gmail.com>",
    "Jiri Spac <capajj@gmail.com>"
  ],
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist/*",
    "copy-dts": "cp dist/src/index.d.ts dist",
    "dist": "npm-run-all clean build copy-dts",
    "test": "jest",
    "prepublish": "npm run dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/webpack-env": "^1.13.6",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "rollup": "^1.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2"
  }
}
