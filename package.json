{
  "name": "graphtype",
  "version": "0.0.5",
  "description": "GraphQL query builder for typescript. Write GQL queries as TS objects and let compiler typecheck your queries",
  "keywords": [
    "graphql",
    "typescript",
    "utility"
  ],
  "repository": "git@github.com:capaj/graphtype",
  "contributors": [
    "gosho-kazuya <ketsume0211@gmail.com>",
    "Jiri Spac <capajj@gmail.com>"
  ],
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c && npm run buildRenderers",
    "buildRenderers": "tsc src/renderGraphTypeClient.ts --outDir dist",
    "clean": "rm -rf dist/*",
    "dist": "npm-run-all clean build",
    "test": "jest",
    "w": "jest --watch",
    "wd": "DEBUG=graphtype jest --watch",
    "prepublishOnly": "npm run dist"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "graphql-codegen-core": "^0.16.1",
    "lodash": "^4.17.11"
  },
  "peerDependencies": {
    "graphql": "^0.13 || ^14"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/graphql": "^14.0.5",
    "@types/jest": "^24.0.0",
    "@types/lodash.get": "^4.4.4",
    "@types/node": "^10.12.21",
    "@types/webpack-env": "^1.13.7",
    "graphql": "^14.1.1",
    "jest": "24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rollup": "^1.1.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.3.1"
  }
}
