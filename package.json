{
  "name": "graphtype",
  "version": "0.0.1",
  "description": "GraphQL query builder for typescript. Write GQL queries as TS objects and let compiler typecheck your queries",
  "keywords": [
    "graphql",
    "typescript",
    "utility"
  ],
  "repository": "git@github.com:capaj/graphtype",
  "contributors": [
    "gosho-kazuya <ketsume0211@gmail.com>",
    "Jiri Spac <capajj@gmail.com>"
  ],
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist/*",
    "copy-dts": "cp dist/src/index.d.ts dist",
    "dist": "npm-run-all clean build copy-dts",
    "test": "jest",
    "w": "jest --watch",
    "prepublishOnly": "npm run dist"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/jest": "^23.3.13",
    "@types/lodash.get": "^4.4.4",
    "@types/node": "^10.12.18",
    "@types/webpack-env": "^1.13.6",
    "jest": "24.0.0-alpha.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.1",
    "rollup": "^1.1.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.2.4"
  }
}
